{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "react/no-multi-comp": 0,
    "react/require-default-props": 0
  },
  "overrides": [
    {
      "files": ["docs/**"],
      "rules": {
        "import/no-extraneous-dependencies": 0
      }
    }
  ]
}
