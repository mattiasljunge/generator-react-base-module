{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/no-multi-comp": "off",
    "react/require-default-props": "off"
  },
  "overrides": [
    {
      "files": ["docs/**"],
      "rules": {
        "import/no-extraneous-dependencies": 0
      }
    }
  ]
}
